                <td class="currency"><strong>{{ formatCurrency(content.totals.transferToGuestLedger.ytd) }}</strong></td>
            </tr>
        </tbody>
    </table>
    
    <!-- Section 6: Payment -->
    <div class="section-title">Payment</div>
    <table>
        <tbody>
            @each(method in Object.keys(content.payments.methods))
            <tr>
                <td>{{ method }}</td>
