<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Management Report</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            font-size: 12px;
            margin: 0;
            padding: 20px;
            color: #333;
        }
        
        .header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 10px;
            font-weight: bold;
            font-size: 14px;
        }
        
        .horizontal-line {
            border-top: 1px solid #333;
            margin: 10px 0;
        }
        
        .report-info {
            margin-bottom: 20px;
            font-size: 11px;
        }
        
        .report-info span {
            margin-right: 30px;
        }
        
        table {
            width: 100%;
            border-collapse: collapse;
            margin-bottom: 15px;
        }
        
        th, td {
            text-align: left;
            padding: 4px 0px;
            font-size: 11px;
        }
        
        th {
            font-weight: bold;
            background-color: #f5f5f5;
        }
        
        .section-title {
            font-weight: bold;
            margin: 15px 0 5px 0;
            font-size: 12px;
        }
        
        .section-total {
            font-weight: bold;
            border-top: 1px dotted #333;
            border-bottom: 1px dotted #333;
        }
        
        .section-subtotal {
            font-weight: bold;
            border-top: 1px solid #ddd;
        }
        
        .bold-section {
            font-weight: bold;
        }
        
        .currency {
            text-align: right;
        }
        
        .footer {
            position: fixed;
            bottom: 20px;
            left: 20px;
            right: 20px;
            font-size: 10px;
        }
        
        .footer-line {
            border-top: 1px solid #333;
            margin-bottom: 5px;
        }
        
        .footer-content {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }
        
        .page-break {
            page-break-before: always;
        }
        
        @media print {
            .page-break {
                page-break-before: always;
            }
        }
    </style>
</head>
<body>
    <!-- Header -->
    <div class="header">
        <span>{{ hotelName }}</span>
        <span>Management Report</span>
    </div>
    <div class="horizontal-line"></div>
    
    <!-- Report Info -->
    <div class="report-info">
        <span><strong>As On Date:</strong> {{ asOnDate }}</span>
        <span><strong>PTD:</strong> {{ ptdDate }}</span>
        <span><strong>YTD:</strong> {{ ytdDate }}</span>
        <span><strong>Currency:</strong> {{ currency }}</span>
    </div>
    <div class="horizontal-line"></div>
    
    <!-- Column Headers -->
    <table>
        <thead>
            <tr>
                <th style="width: 40%;">Particulars</th>
                <th style="width: 20%;">Today ({{ currency }})</th>
                <th style="width: 20%;">PTD ({{ currency }})</th>
                <th style="width: 20%;">YTD ({{ currency }})</th>
            </tr>
        </thead>
    </table>
    <div class="horizontal-line"></div>

    <!-- Section 1: Room Charges -->
    <div class="section-title">Room Charges</div>
    <table>
        <tbody>
            <tr>
                <td style="width: 40%;">Room Charges</td>
                <td class="currency" style="width: 20%;">{{ formatCurrency(content.roomCharges?.roomCharges?.today) }}</td>
                <td class="currency" style="width: 20%;">{{ formatCurrency(content.roomCharges?.roomCharges?.ptd) }}</td>
                <td class="currency" style="width: 20%;">{{ formatCurrency(content.roomCharges?.roomCharges?.ytd) }}</td>
            </tr>
            <tr>
                <td>Cancellation Revenue</td>
                <td class="currency">{{ formatCurrency(content.roomCharges?.cancellationRevenue.today) }}</td>
                <td class="currency">{{ formatCurrency(content.roomCharges?.cancellationRevenue.ptd) }}</td>
                <td class="currency">{{ formatCurrency(content.roomCharges?.cancellationRevenue.ytd) }}</td>
            </tr>
            <tr>
                <td>No Show Revenue</td>
                <td class="currency">{{ formatCurrency(content.roomCharges?.noShowRevenue.today) }}</td>
                <td class="currency">{{ formatCurrency(content.roomCharges?.noShowRevenue.ptd) }}</td>
                <td class="currency">{{ formatCurrency(content.roomCharges?.noShowRevenue.ytd) }}</td>
            </tr>
            <tr class="section-total">
                <td><strong>Total Room Charges</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.roomCharges?.total.today) }}</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.roomCharges?.total.ptd) }}</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.roomCharges?.total.ytd) }}</strong></td>
            </tr>
        </tbody>
    </table>
    
    <!-- Section 2: Extra Charges -->
    <div class="section-title">Extra Charges</div>
    <table>
        <tbody>
            @each(extraCharge in Object.keys(content.extraCharges.extraCharges || {}))
            <tr>
                <td>{{ extraCharge }}</td>
                <td class="currency">{{ formatCurrency(content.extraCharges.extraCharges[extraCharge].today) }}</td>
                <td class="currency">{{ formatCurrency(content.extraCharges.extraCharges[extraCharge].ptd) }}</td>
                <td class="currency">{{ formatCurrency(content.extraCharges.extraCharges[extraCharge].ytd) }}</td>
            </tr>
            @endeach
            <tr class="section-total">
                <td><strong>Total Extra Charges</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.extraCharges.totals?.today || content.extraCharges.today) }}</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.extraCharges.totals?.ptd || content.extraCharges.ptd) }}</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.extraCharges.totals?.ytd || content.extraCharges.ytd) }}</strong></td>
            </tr>
        </tbody>
    </table>
    
    <!-- Section 3: Discount -->
    <div class="section-title">Discount</div>
    <table>
        <tbody>
            @each(discount in (content.discounts.discounts || []))
            <tr>
                <td>{{ discount.name }} ({{ discount.shortCode }})</td>
                <td class="currency">{{ formatCurrency(discount.today) }}</td>
                <td class="currency">{{ formatCurrency(discount.ptd) }}</td>
                <td class="currency">{{ formatCurrency(discount.ytd) }}</td>
            </tr>
            @endeach
            <tr class="section-total">
                <td><strong>Total Discount</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.discounts.totals?.today || content.discounts.today) }}</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.discounts.totals?.ptd || content.discounts.ptd) }}</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.discounts.totals?.ytd || content.discounts.ytd) }}</strong></td>
            </tr>
        </tbody>
    </table>
    
    <!-- Section 4: Adjustments -->
    <div class="section-title">Adjustments</div>
    <table>
        <tbody>
            @each(adjustment in (content.adjustments.adjustments || []))
            <tr>
                <td>{{ adjustment.name }}</td>
                <td class="currency">{{ formatCurrency(adjustment.today) }}</td>
                <td class="currency">{{ formatCurrency(adjustment.ptd) }}</td>
                <td class="currency">{{ formatCurrency(adjustment.ytd) }}</td>
            </tr>
            @endeach
            <tr class="section-total">
                <td><strong>Total Adjustments</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.adjustments.totals?.today || content.adjustments.today) }}</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.adjustments.totals?.ptd || content.adjustments.ptd) }}</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.adjustments.totals?.ytd || content.adjustments.ytd) }}</strong></td>
            </tr>
        </tbody>
    </table>
    
    <!-- Section 5: Tax -->
    <div class="section-title">Tax</div>
    <table>
        <tbody>
            @each(taxRate in ( content.tax.taxes || []))
            <tr>
                <td>{{ taxRate.name }}</td>
                <td class="currency">{{ formatCurrency(taxRate.today.total) }}</td>
                <td class="currency">{{ formatCurrency(taxRate.ptd.total) }}</td>
