              <td class="currency" style="width: 20%;">
                {{ formatCurrency(adjustment.ytd) }}
              </td>
            </tr>
          @endeach
          <tr class="section-total">
            <td>
              <strong>Total Adjustments</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.adjustments.totals?.today || content.adjustments.today) }}</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.adjustments.totals?.ptd || content.adjustments.ptd) }}</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.adjustments.totals?.ytd || content.adjustments.ytd) }}</strong>
            </td>
          </tr>
        </tbody>
      </table>

      <!-- Section 5: Tax -->
      <div class="section-title">
        Tax
      </div>
      <table>
        <tbody>
          @each(taxRate in ( content.tax.taxes || []))
            <tr>
              <td style="width: 40%;">
                {{ taxRate.name }}
              </td>
              <td class="currency" style="width: 20%;">
                {{ formatCurrency(taxRate.today) }}
              </td>
              <td class="currency" style="width: 20%;">
                {{ formatCurrency(taxRate.ptd) }}
              </td>
              <td class="currency" style="width: 20%;">
