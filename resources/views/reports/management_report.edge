                {{ formatCurrency(content.payments.methods[method].ptd) }}
              </td>
              <td class="currency" style="width: 20%;">
                {{ formatCurrency(content.payments.methods[method].ytd) }}
              </td>
            </tr>
          @end
          <tr class="section-total">
            <td>
              <strong>Total Payment</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.payments.total.today) }}</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.payments.total.ptd) }}</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.payments.total.ytd) }}</strong>
            </td>
          </tr>
        </tbody>
      </table>

      <!-- Section 7: City Ledger -->
      <div class="section-title">
        City Ledger
      </div>
      <table>
        <tbody>
          <tr>
            <td style="width: 40%;">
              Opening Balance
            </td>
            <td class="currency" style="width: 20%;">
              {{ formatCurrency(content.cityLedger?.openingBalance?.today) }}
            </td>
            <td class="currency" style="width: 20%;">
