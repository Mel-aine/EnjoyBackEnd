<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>
      Manager Report
    </title>
    <style>
      body {
        font-family: Arial, sans-serif;
        font-size: 12px;
        margin: 0;
        padding: 20px;
        color: #333;
      }
      
      .header {
        position: fixed;
        top: 0;
        display: flex;
        justify-content: space-between;
        align-items: center;
        margin-bottom: 10px;
        font-weight: bold;
        font-size: 14px;
      }
      
      .horizontal-line {
        border-top: 1px solid #333;
        margin: 10px 0;
      }
      
      .report-info {
        margin-bottom: 20px;
        font-size: 11px;
      }
      
      table {
        width: 100%;
        border-collapse: collapse;
        margin-bottom: 15px;
      }
      
      th,
      td {
        text-align: left;
        padding: 4px 0px;
        font-size: 11px;
      }
      
      th {
        font-weight: bold;
        background-color: #f5f5f5;
      }
      
      .section-title {
        font-weight: bold;
        margin: 15px 0 5px 0;
        font-size: 12px;
      }
      
      .section-total {
        font-weight: bold;
        border-top: 1px dotted #333;
        border-bottom: 1px dotted #333;
      }
      
      .section-total-top {
        font-weight: bold;
        border-top: 1px dotted #333;
      }
      
      .section-total-bottom {
        font-weight: bold;
        border-bottom: 1px dotted #333;
      }
      
      .section-subtotal {
        font-weight: bold;
        border-top: 1px solid #ddd;
      }
      
      .bold-section {
        font-weight: bold;
      }
      
      .currency {
        text-align: right;
      }
      
     
      .footer-line {
        border-top: 1px solid #333;
        margin-bottom: 5px;
      }
      
      .footer-content {
        display: flex;
        justify-content: space-between;
        align-items: center;
      }
      
      
      .font-bold{
        font-weight: 900;
      }
      @media print {
        .page-break {
          padding-top:20px
          page-break-before: always;
        }
      
        /* Ensure header is at the top */
      
        .header {
          position: absolute;
          top: 0;
          left: 0;
          right: 0;
          height: 50px;
          padding: 10px 20px;
          /* Adjust height as needed */
          background-color: white;
          /* To prevent background show-through */
        }
      
        /* Ensure footer is at the bottom */
      
        .footer {
          height: 50px;
          /* Adjust height as needed */
          background-color: white;
          /* To prevent background show-through */
        }
      
        /* Optional: Add padding to the body to prevent content overlap */
      
        .main {
          position:absolute
          padding-top: 20px;
          /* Add space for the header */
          padding-bottom: 80px;
          /* Add space for the footer */
        }
      }
    </style>
  </head>
  <body>
    <!-- Header -->
    <div class="header">
      <span>{{ hotelName }}</span>
      <span>Manager Report</span>
    </div>
    <div class="main">
      <div class="horizontal-line">
      </div>

      <!-- Report Info -->
      <div class="report-info">
        <span>          <strong>As On Date:</strong>{{ asOnDate }}</span>
        <span>          <strong>PTD:</strong>{{ ptdDate }}</span>
        <span>          <strong>YTD:</strong>{{ ytdDate }}</span>
        <span>          <strong>Currency:</strong>{{ currency }}</span>
      </div>
      <div class="horizontal-line">
      </div>

      <!-- Column Headers -->
      <table>
        <thead>
          <tr>
            <th style="width: 40%;">
              Particulars
            </th>
            <th class="currency" style="width: 20%;">
              Today
            </th>
            <th class="currency" style="width: 20%;">
              PTD
            </th>
            <th class="currency" style="width: 20%;">
              YTD
            </th>
          </tr>
        </thead>
      </table>
      <div class="horizontal-line">
      </div>

      <!-- Section 1: Room Charges -->
      <div class="section-title">
        Room Charges
      </div>
      <table>
        <tbody>
          <tr>
            <td style="width: 40%;">
              Room Charges
            </td>
            <td class="currency" style="width: 20%;">
              {{ formatCurrency(content.roomCharges?.roomCharges?.today) }}
            </td>
            <td class="currency" style="width: 20%;">
              {{ formatCurrency(content.roomCharges?.roomCharges?.ptd) }}
            </td>
            <td class="currency" style="width: 20%;">
              {{ formatCurrency(content.roomCharges?.roomCharges?.ytd) }}
            </td>
          </tr>
          <tr>
            <td>
              Cancellation Revenue
            </td>
            <td class="currency">
              {{ formatCurrency(content.roomCharges?.cancellationRevenue.today) }}
            </td>
            <td class="currency">
              {{ formatCurrency(content.roomCharges?.cancellationRevenue.ptd) }}
            </td>
            <td class="currency">
              {{ formatCurrency(content.roomCharges?.cancellationRevenue.ytd) }}
            </td>
          </tr>
          <tr>
            <td>
              No Show Revenue
            </td>
            <td class="currency">
              {{ formatCurrency(content.roomCharges?.noShowRevenue.today) }}
            </td>
            <td class="currency">
              {{ formatCurrency(content.roomCharges?.noShowRevenue.ptd) }}
            </td>
            <td class="currency">
              {{ formatCurrency(content.roomCharges?.noShowRevenue.ytd) }}
            </td>
          </tr>
          <tr class="section-total">
            <td>
              <strong>Total Room Charges</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.roomCharges?.total.today) }}</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.roomCharges?.total.ptd) }}</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.roomCharges?.total.ytd) }}</strong>
            </td>
          </tr>
        </tbody>
      </table>

      <!-- Section 2: Extra Charges -->
      <div class="section-title">
        Extra Charges
      </div>
      <table>
        <tbody>
          @each(extraCharge in Object.keys(content.extraCharges.extraCharges || {}))
            <tr>
              <td style="width: 40%;">
                {{ extraCharge }}
              </td>
              <td class="currency" style="width: 20%;">
                {{ formatCurrency(content.extraCharges.extraCharges[extraCharge].today) }}
              </td>
              <td class="currency" style="width: 20%;">
                {{ formatCurrency(content.extraCharges.extraCharges[extraCharge].ptd) }}
              </td>
              <td class="currency" style="width: 20%;">
                {{ formatCurrency(content.extraCharges.extraCharges[extraCharge].ytd) }}
              </td>
            </tr>
          @endeach
          <tr class="section-total">
            <td>
              <strong>Total Extra Charges</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.extraCharges.totals?.today || content.extraCharges.today) }}</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.extraCharges.totals?.ptd || content.extraCharges.ptd) }}</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.extraCharges.totals?.ytd || content.extraCharges.ytd) }}</strong>
            </td>
          </tr>
        </tbody>
      </table>

      <!-- Section 3: Discount -->
      <div class="section-title">
        Discount
      </div>
      <table>
        <tbody>
          @each(discount in (content.discounts.discounts || []))
            <tr>
              <td style="width: 40%">
                {{ discount.name }} ({{ discount.shortCode }})
              </td>
              <td class="currency" style="width: 20%;">
                {{ formatCurrency(discount.today) }}
              </td>
              <td class="currency" style="width: 20%;">
                {{ formatCurrency(discount.ptd) }}
              </td>
              <td class="currency" style="width: 20%;">
                {{ formatCurrency(discount.ytd) }}
              </td>
            </tr>
          @endeach
          <tr class="section-total">
            <td>
              <strong>Total Discount</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.discounts.totals?.today || content.discounts.today) }}</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.discounts.totals?.ptd || content.discounts.ptd) }}</strong>
            </td>
            <td class="currency">
              <strong>{{ formatCurrency(content.discounts.totals?.ytd || content.discounts.ytd) }}</strong>
            </td>
          </tr>
        </tbody>
      </table>

      <!-- Section 4: Adjustments -->
      <div class="section-title">
        Adjustments
      </div>
      <table>
        <tbody>
          @each(adjustment in (content.adjustments.adjustments || []))
