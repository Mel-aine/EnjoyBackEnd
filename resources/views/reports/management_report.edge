                <td class="currency"><strong>{{ formatCurrency(content.tax.totals?.today || content.tax.today) }}</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.tax.totals?.ptd || content.tax.ptd) }}</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.tax.totals?.ytd || content.tax.ytd) }}</strong></td>
            </tr>
        </tbody>
    </table>
    
    <!-- Bold Section After Tax -->
    <table>
        <tbody>
            <tr class="bold-section">
                <td><strong>Total Revenue without Tax</strong></td>
                <td class="currency"><strong>{{ formatCurrency(content.totals.revenueWithoutTax.today) }}</strong></td>
